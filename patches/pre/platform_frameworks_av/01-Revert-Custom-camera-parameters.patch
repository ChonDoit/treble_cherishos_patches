         "CaptureResult.cpp",
         "CameraParameters2.cpp",
         "CameraSessionStats.cpp",
@@ -137,16 +137,6 @@ cc_library_host_static {
     ],
 }
 
-cc_library_static {
-    name: "libcamera_parameters",
-
-    export_include_dirs: [
-        "include",
-    ],
-    srcs: ["CameraParameters.cpp"],
-
-}
